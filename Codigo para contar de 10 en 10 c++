#include <iostream>

using namespace std;

int main()
{
    int x[101];
    int y=0;
    cout << "Mostrar los numeros del 1 al 1000" << endl;

    for ( y=1 ; y < 101 ; y++ ){
        x[y]=y*10;
        }
    for (y=1; y<101; y++){
        cout<<"El numero en la posision "<< y <<"es: "<<x[y]<<endl;
    }
    return 0;
}
